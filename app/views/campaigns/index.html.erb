<h1>Campaigns</h1>

<table class="table">
  <thead>
    <tr>
      <th>Campaign ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campaigns.each do |campaign| %>
      <tr>
        <td><%= campaign.campaign %></td>
        <td><%= link_to 'View', campaign %></td>
      </tr>
    <% end %>
  </tbody>
</table>
